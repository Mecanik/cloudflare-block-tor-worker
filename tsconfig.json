{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types",
      "@types/jest",
      "@types/service-worker-mock",
	  "@types/node"
    ],
	"noImplicitAny": false,
	"strictNullChecks": false,
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "test"]
}
